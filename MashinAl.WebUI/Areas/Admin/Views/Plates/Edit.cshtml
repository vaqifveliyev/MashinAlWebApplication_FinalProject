@using MashinAl.Business.Modules.PlateModule.Queries.PlateGetByIdQuery
@model PlateGetByIdDto

@{
    ViewData["Title"] = Model.PlateNumber + " | Dəyişiklik et";
}


<div class="card">
    <div class="card-body">
        <h5 class="card-title fw-semibold mb-4">@Model.PlateNumber | Ətraflı </h5>
        <div class="card">
            <div class="card-body">
                <form method="post">
                    <div class="mb-3">
                        <label class="form-label">Region</label>
                        <select asp-for="RegionId" asp-items="@ViewBag.RegionId" class="form-control">
                            <option disabled selected>@Model.RegionId</option>
                        </select>
                        <label class="form-label">Seriya</label>
                        <select asp-for="@Model.FirstLetter" class="form-control">
                            <option selected disabled>@Model.FirstLetter</option>
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                            <option value="D">D</option>
                            <option value="E">E</option>
                            <option value="F">F</option>
                            <option value="G">G</option>
                            <option value="H">H</option>
                            <option value="J">J</option>
                            <option value="K">K</option>
                            <option value="L">L</option>
                            <option value="M">M</option>
                            <option value="N">N</option>
                            <option value="P">P</option>
                            <option value="R">R</option>
                            <option value="S">S</option>
                            <option value="T">T</option>
                            <option value="U">U</option>
                            <option value="V">V</option>
                            <option value="X">X</option>
                            <option value="Y">Y</option>
                            <option value="Z">Z</option>
                        </select>
                        <select asp-for="@Model.SecondLetter" class="form-control">
                            <option selected disabled>@Model.SecondLetter</option>
                            <option value="A">A</option>
                            <option value="B">B</option>
                            <option value="C">C</option>
                            <option value="D">D</option>
                            <option value="E">E</option>
                            <option value="F">F</option>
                            <option value="G">G</option>
                            <option value="H">H</option>
                            <option value="J">J</option>
                            <option value="K">K</option>
                            <option value="L">L</option>
                            <option value="M">M</option>
                            <option value="N">N</option>
                            <option value="P">P</option>
                            <option value="R">R</option>
                            <option value="S">S</option>
                            <option value="T">T</option>
                            <option value="U">U</option>
                            <option value="V">V</option>
                            <option value="X">X</option>
                            <option value="Y">Y</option>
                            <option value="Z">Z</option>
                        </select><br>
                        <label class="form-label">Qeydiyyat nişanı</label>
                        <input class="form-control" type="text" asp-for="@Model.PlateNumber" value="@Model.PlateNumber"/>
                        <label class="form-label">Sahibi</label>
                        <input class="form-control" type="text" asp-for="@Model.Name" value="@Model.Name" />
                        <label class="form-label">Email</label>
                        <input class="form-control" type="text" asp-for="@Model.Email" value="@Model.Email" />
                        <label class="form-label">Əlaqə nömrəsi</label>
                        <input class="form-control" type="text" asp-for="@Model.Phone" value="@Model.Phone" />
                        <label class="form-label">Satış şəhəri</label>
                        <select asp-for="CityId" asp-items="@ViewBag.CityId" class="form-control">
                            <option disabled selected>@Model.CityId</option>
                        </select>
                        <label class="form-label">Qiymət</label>
                        <input class="form-control" type="text" asp-for="@Model.Price" value="@Model.Price" />
                    </div>
                    <a asp-action="index" class="btn btn-primary">Geri</a>
                    <button type="submit" class="btn btn-success">Yadda saxla</button>
                </form>
            </div>
        </div>
    </div>
</div>

